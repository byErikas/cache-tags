{
    "name": "byerikas/cache-tags",
    "description": "Allows for Redis/Valkey cache flushing multiple tagged items by a single tag.",
    "keywords": [
        "laravel",
        "cache",
        "valkey",
        "redis",
        "tags"
    ],
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/cache": "^10|^11",
        "illuminate/redis": "^10|^11",
        "illuminate/support": "^10|^11"
    },
    "require-dev": {
        "laravel/framework": "^10|^11",
        "pestphp/pest": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "ByErikas\\CacheTags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "byErikas",
            "email": "erikasbystautas1@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "ByErikas\\CacheTags\\ServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}